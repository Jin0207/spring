package com.sist.exam02;

public class Dept {
	private int dno;
	private String dname;
	private String dloc;
	
	public Dept() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Dept(int dno, String dname, String dloc) {
		super();
		this.dno = dno;
		this.dname = dname;
		this.dloc = dloc;
	}
	
	
}
